{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": [
    "flowtype"
  ],
  rules: {
    "arrow-parens": [2, "always"],
    "no-console": [0],
    "import/no-extraneous-dependencies": [0],
    "consistent-return": [0],
    "import/no-dynamic-require": [0],
    "arrow-body-style": [0],
    "no-nested-ternary": [0]
  }
}