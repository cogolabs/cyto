{
  "name": "cyto",
  "version": "0.0.1",
  "description": "A better boilerplate generator",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack && npm run cleanup",
    "build-prod": "cross-env NODE_ENV=production webpack -p && npm run cleanup",
    "cleanup": "mv bin/cyto.js bin/cyto && chmod +x bin/*",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "jest"
  },
  "keywords": [],
  "author": "Connor Taylor",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^18.1.0",
    "webpack": "^1.14.0"
  }
}
