{
  "presets": [
    ["env", {
      "targets": {
        "node": 4
      },
      "modules": false
    }]
  ],
  "env": {
    "test": {
      presets: [["env", { "targets": { "node": 4 } }]]
    }
  },
  "plugins": [
    "transform-flow-strip-types",
    "transform-runtime",
    "transform-async-to-generator",
    "transform-object-rest-spread"
  ]
}